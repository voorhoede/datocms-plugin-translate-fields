{
  "name": "datocms-plugin-translate-fields",
  "homepage": "https://github.com/voorhoede/datocms-plugin-translate-fields",
  "version": "1.9.0",
  "description": "A DatoCMS plugin that gives you the ability to translate `structured-text`, `rich-text`, `string`, `text`, `seo` and `slug` fields in DatoCMS",
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "dev": "cross-env REACT_APP_USE_MOCK=true npm run start",
    "start": "cross-env BROWSER='none' PUBLIC_URL='/' react-scripts start",
    "build": "cross-env PUBLIC_URL='.' react-scripts build",
    "test": "jest",
    "test:watch": "jest --watch",
    "eject": "react-scripts eject",
    "prepack": "npm run build",
    "postversion": "git push --follow-tags"
  },
  "dependencies": {
    "@types/jest": "^29.5.1",
    "@types/lodash": "^4.14.194",
    "@types/node": "^20.2.3",
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.4",
    "datocms-plugin-sdk": "^0.7.13",
    "datocms-react-ui": "^0.7.13",
    "datocms-structured-text-slate-utils": "^2.1.2",
    "datocms-structured-text-utils": "^2.0.4",
    "html2json": "^1.0.2",
    "mdast-util-from-markdown": "^1.3.0",
    "mdast-util-to-markdown": "^1.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "slugify": "^1.6.6",
    "typescript": "^4.9.5"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "jest": "^29.5.0",
    "jest-fetch-mock": "^3.0.3",
    "react-scripts": "^5.0.1",
    "ts-jest": "^29.1.0"
  },
  "files": [
    "build",
    "docs"
  ],
  "keywords": [
    "datocms",
    "datocms-plugin",
    "field-addon",
    "translate",
    "text",
    "string",
    "structured-text",
    "rich-text",
    "modular-content",
    "markdown",
    "slug",
    "deepl",
    "yandex",
    "seo",
    "chatGPT",
    "openAI",
    "GPT-3"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/voorhoede/datocms-plugin-translate-fields.git"
  },
  "author": "Voorhoede",
  "license": "ISC",
  "datoCmsPlugin": {
    "title": "Translate",
    "previewImage": "docs/preview.png",
    "coverImage": "docs/cover.png",
    "entryPoint": "build/index.html"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
